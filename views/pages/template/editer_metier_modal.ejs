<div class="modal fade" id="editer_metier_modal" tabindex="-1" role="dialog" aria-labelledby="editer_metier_modal"
    aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="editer_metier_modal">Modification Metier</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">



                <form>
                    <div class="form-group">
                        <label for="editer-code-metier " class="col-form-label">code metier:</label>
                        <input type="text" maxlength="7" minlength="6" class="form-control edit-code-met"
                            id="editer-code_metier" required readonly>

                    </div>
                    <div class="form-group">
                        <label for="editer-design-metier" class="col-form-label">Designation metier:</label>
                        <input type="text" maxlength="25" minlength="4" class="form-control edit-design-met"
                            id="editer-design-metier" required>
                    </div>




                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" onclick="modifier_metier()">Modifier metier</button>
            </div>
        </div>
    </div>
</div>