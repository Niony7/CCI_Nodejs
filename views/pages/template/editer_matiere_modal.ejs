<div class="modal fade" id="editer_matiere_modal" tabindex="-1" role="dialog" aria-labelledby="editer_matiere-modal"
    aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="editer_matiere_modal">Modification Matiere</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form>

                    <div class="form-group">
                        <label for="editer-code-matiere" class="col-form-label">code Matiere:</label>
                        <input type="text" class="form-control" id="editer-code-matiere" required readonly>

                    </div>
                    <div class="form-group">
                        <label for="code-metier-mat-edt" class="col-form-label">Code Metier:</label>
                        <select type="text" class="form-control" id="code-metier-mat-edt" class="form-control" readonly>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="editer-nom-matiere" class="col-form-label">Nom Matiere:</label>
                        <input type="text" maxlength="50" minlength="4" class="form-control" id="editer-nom-matiere"
                            required>
                    </div>

                    <div class="form-group">
                        <label for="editer-note-matiere" class="col-form-label">Note matiere:</label>
                        <input type="double" max="5" min="1" class="form-control" id="editer-note-matiere" required>
                    </div>

                    <div class="form-group">
                        <label for="editer-coef-matiere" class="col-form-label">Coeficiant matiere:</label>
                        <input type="double" max="4" min="1" class="form-control" id="editer-coef-matiere" required>
                    </div>

                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" onclick="modifier_matiere()">Modifier matiere</button>
            </div>
        </div>
    </div>
</div>